@model List<PreAgendamento>
@{
    ViewData["Title"]= "Agendamento concluído";
}

@foreach (PreAgendamento info in Model)
{
   <h3> @info.seunome , anote aí o que você agendou:</h3>
}
<body style="background-color: lemonchiffon; text-align: center;">
<br>
<table class="table table-striped table-bordered table-condensed table-hover">
    <thead>
        <tr>
            <th>Nome do animal 👇</th>
            <th>Telefone 📞</th>
            <th>Dia e hora 📆</th>
            <th>Animal 🐱🐶</th>
            <th>Necessidade 🆘</th>
        </tr>
    </thead>
    <tbody>
        @foreach (PreAgendamento info in Model)
        {
            <tr>
                <td class="active">@info.nomeanimal</td>
                <td class="success">@info.telefone</td>
                <td class="danger">@info.data</td>
                <td class="warning">@info.animal</td>
                <td class="info">@info.necessidade</td>
            </tr>
        }
    </tbody>
</table>
<img src="~/img/anota_ai.jpg" width="400">
</body>

